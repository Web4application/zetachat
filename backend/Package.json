{
  "name": "zetachat",
  "version": "2.0.0",
  "description": "Zetachat is a chat application with AI-powered features, real-time messaging, and social connectivity.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "react-scripts start",
    "server": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "react-scripts build",
    "test:client": "jest --watch src",
    "test:server": "jest --watch server",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,json,css}\"",
    "monitor": "nodemon server.js",
    "start:full": "concurrently \"cd backend && npm start\" \"cd frontend && npm start\"",
    "start:production": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^18.0.3",
    "express": "^4.18.2",
    "helmet": "^6.1.0",
    "cors": "^2.8.5",
    "openai": "^3.2.1",
    "socket.io": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.6.0",
    "redux": "^4.2.0",
    "react-redux": "^8.1.1",
    "redux-thunk": "^2.4.2",
    "mongoose": "^7.4.0",
    "pg": "^8.11.0",
    "sequelize": "^6.32.1",
    "web-push": "^3.4.0",
    "chart.js": "^4.4.0",
    "styled-components": "^5.3.11",
    "material-ui": "^5.13.4",
    "nlp.js": "^4.20.0",
    "tensorflow": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "eslint": "^8.44.0",
    "eslint-plugin-react": "^7.32.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.2",
    "webpack": "^5.89.0",
    "babel-loader": "^9.1.2",
    "terser-webpack-plugin": "^5.4.1"
  },
  "author": "Kubu Lee",
  "license": "MIT"
}
